{
  "name": "Darts Roomly Extension",
  "description": "Chrome extension to use for playing dart @ roomly",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": [
    "http://localhost:8082/*",
    "http://192.168.1.171:8082/*"
  ],
  "content_scripts": [
    {
      "js": ["scripts/index.js"],
      "matches": ["https://play.autodarts.io/*"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["*://*/*"]
    }
  ],
  "action": {
    "default_popup": "extension.html",
    "default_icon": "extension.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "roomly-darts-extension@nvier.deinemum",
      "strict_min_version": "109.0"
    }
  }
}
